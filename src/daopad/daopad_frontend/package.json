  "private": true,
  "scripts": {
    "build": "npm run test:toast && vite build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "prebuild": "node -e \"console.log('Skipping dfx generate: declarations vendored locally')\"",
    "setup": "npm i && dfx canister create daopad_backend && dfx generate daopad_backend && dfx deploy",
    "start": "vite --port 3000",
    "dev": "vite --port 3000",
    "test": "vitest run",
    "test:unit": "vitest run src/__tests__/unit src/services/__tests__ src/features src/utils",
    "test:integration": "vitest run src/__tests__/integration --reporter=verbose",
    "test:mainnet": "npm run test:integration",
    "test:toast": "node test-toast-integration.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "playwright test e2e/setup/auth.setup.ts --headed",
    "verify:deployment": "node verify-deployment.mjs"
  },
  "type": "module",
  "version": "0.0.0"
